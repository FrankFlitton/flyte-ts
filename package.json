{
  "name": "flyte-ts-client",
  "description": "Flyte client for TypeScript",
  "private": false,
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "main": "./dist/main.umd.cjs",
  "module": "./dist/main.js",
  "typings": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate": "openapi --input ./public/openapi.json --output ./generated"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "buffer": "^6.0.3",
    "openapi-typescript-codegen": "^0.25.0",
    "picocolors": "^1.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-progress": "^0.0.7"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "node-fetch": "^2.7.0",
    "node-self": "^1.0.2",
    "portable-fetch": "^3.0.0",
    "url": "^0.11.3",
    "xmlhttprequest": "^1.8.0"
  }
}
